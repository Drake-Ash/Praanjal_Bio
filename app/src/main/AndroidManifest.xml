<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.ash.demoapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ClientRegActivity" />
        <activity android:name=".PatientListActivity" />
        <activity android:name=".PatientRegActivity" />
        <activity android:name=".PatientView" />
        <activity android:name=".UpdateUser" />
        <activity android:name=".UpdateClient" />
        <activity android:name=".TreatmentEntry" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".ViewCamera" />
        <activity android:name=".OTP_Verification"></activity>
    </application>

</manifest>